    "test":"webpack --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/inkroom/music.git"
  },
  "keywords": [
    "music",
    "electron"
  ],
  "author": "inkbox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inkroom/music/issues"
  },
  "homepage": "https://github.com/inkroom/music#readme",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "electron": "^4.0.1",
    "http-server": "^0.11.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "asynckit": "^0.4.0",
    "axios": "^0.18.0",
