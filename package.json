{
  "name": "music",
  "version": "1.0.0",
  "description": "跨平台，多来源音乐播放器",
  "main": "src/index.js",
  "scripts": {
    "dev": "electron . dev",
    "proc": "node node_modules/less/bin/lessc ./src/css/*.less && electron .",
    "dev-server": "node ./src/dev.js",
    "http-server": "node node_modules/http-server/bin/http-server -p 8001 -c-1",
